@model DownloaderWeb.Models.DownloadMetaData
@{
    ViewData["Title"] = "Download";
  
}

<h2>Download</h2>

@using (Html.BeginForm("DownloadFile","Download", FormMethod.Post))
{
    <div class="form-horizontal">
       <div class="form-group">
           @Html.EditorFor(model => model.downloadUrl, new { htmlAttributes = new { @class = "form-control" } })
       </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Register" class="btn btn-default" />
            </div>
        </div>
       
    </div>
}



